   "source": [
    "def arrangeCoins(n):\n",
    "    k = 0\n",
    "    count = 0\n",
    "\n",
    "    while k * (k + 1) // 2 <= n:\n",
    "        k += 1\n",
    "        n -= k\n",
    "        count += 1\n",
    "\n",
    "    return count\n",
    "n = 5\n",
    "print(arrangeCoins(n))\n"
   ]
